{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "declaration": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "paths": {
      "@polkadot/api/augment": ["./node_modules/@acala-network/types/interfaces/augment-api.d.ts"],
      "@polkadot/types/augment": ["./node_modules/@acala-network/types/interfaces/augment-types.d.ts"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
